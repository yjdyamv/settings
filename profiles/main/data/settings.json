	"cmake.debugConfig": {
		"type": "lldb",
		// "MIMode": "lldb", // 哥们，必不可少啊，不然默认是gdb(~~呜呜，有点玄幻)
		"cwd": "${workspaceFolder}",
		"stopOnEntry": true, // 在代码开头设置断点
		"environment": [],
		"console": "integratedTerminal",
		"setupCommands": [
			{
				"description": "Set assembly code display to Intel syntax format",
				"text": "settings set target.x86-disassembly-flavor intel",
				"ignoreFailures": true
			},
			{
				"description": "Customize stack frame format to include function name, arguments, and file location",
				"text": "settings set frame-format \"frame #${frame.index}: ${frame.pc}{ ${module.file.basename}{`${function.name-with-args}{${frame.no-debug}${function.pc-offset}}}}{ at ${line.file.basename}:${line.number}}{${function.is-optimized} [opt]}\\n\"",
				"ignoreFailures": true
			},
			{
				"description": "Set thread format to display thread ID, name, and current execution position",
				"text": "settings set thread-format \"thread #${thread.index}: tid = ${thread.id%tid}{, ${frame.pc}}{ ${module.file.basename}{`${function.name-with-args}{${frame.no-debug}${function.pc-offset}}}}{ at ${line.file.basename}:${line.number}}{, name = '${thread.name}'}\\n\"",
				"ignoreFailures": true
			},
			{
				"description": "Enable detailed type information display for dynamic objects",
				"text": "settings set target.prefer-dynamic-value run-target",
				"ignoreFailures": true
			},
			{
				"description": "Set maximum string display length to 512 characters",
				"text": "settings set target.max-string-summary-length 512",
				"ignoreFailures": true
			}
		]
	},
	// // 2. 适用于MSVC的调试
	// // 使用MSVC的调试器
	// "cmake.debugConfig": {
	// 	"type": "cppvsdbg", // 使用VS的调试器
	// 	"cwd": "${workspaceFolder}",
	// 	"environment": [],
	// 	"console": "integratedTerminal",
	// },
	// lldb插件相关设置
	// 启动时执行的初始化命令（在目标程序启动前执行）
	"lldb.launch.initCommands": [],
	// 运行前执行的命令（在目标程序启动后、断点命中前执行）
	"lldb.launch.preRunCommands": [],
	// 自动解引用指针，显示指针指向的实际值而不是地址
	"lldb.dereferencePointers": true,
	// 抑制缺失源文件的警告信息，避免调试时的干扰
	"lldb.suppressMissingSourceFiles": true,
	// 反汇编显示模式：never=从不显示，auto=自动，always=总是显示
	"lldb.showDisassembly": "never",
	// 启用详细日志记录，便于调试LLDB本身的问题
	"lldb.verboseLogging": true,
	// 启用命令自动补全功能，提高调试效率
	"lldb.commandCompletions": true,
	// 控制台模式：split=分割模式，可以直接在Debug Console中输入变量名查看值
	// 其他选项：commands=仅命令模式，evaluate=仅求值模式
	"lldb.consoleMode": "split",
	"lldb.displayFormat": "auto", // 变量显示格式
	"lldb.launch.expressions": "simple", // 表达式求值模式
	
	
	// python languageServer
	"python.languageServer": "None",
	"python.analysis.autoImportCompletions": true, // 启用自动导入补全
	"python.analysis.indexing": true, // 启用索引以提高性能
	"python.analysis.inlayHints.variableTypes": true, // 显示变量类型的内联提示（在变量旁边显示其类型信息）
	"python.analysis.inlayHints.functionReturnTypes": true, // 显示函数返回值类型的内联提示（在函数定义处显示返回类型）
	"python.analysis.inlayHints.callArgumentNames": "all", // 显示函数调用时所有参数名称的内联提示（帮助理解参数含义）
	"ruff.exclude": [
		// 排除的文件或目录
	],
	"ruff.lint.ignore": [
		// 忽略的规则列表
		"F401" // 忽略未使用的导入
	],
	"ty.logLevel": "debug",
	
	// Python配置 默认使用ruff格式化(记得在环境里装好ruff) `pip install ruff`
	"[python]": {
		"editor.inlayHints.enabled": "on",
		"editor.formatOnType": true,
		// 保存文件就格式化代码
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "charliermarsh.ruff",
		"editor.codeActionsOnSave": {
			"source.fixAll": "explicit",
			"source.organizeImports": "explicit"
		}
	},
	// jupyter配置
	"notebook.formatOnSave.enabled": true,
	"notebook.codeActionsOnSave": {
		"notebook.source.fixAll": "explicit",
		"notebook.source.organizeImports": "explicit"
	},
	"notebook.lineNumbers": "on", // 显示行号
	"notebook.showCellStatusBar": "visible", // 显示单元格状态栏


	// typst
	"tinymist.formatterMode": "typstyle",
	"tinymist.lint.enabled": true, 
	"[typst]": {
		"editor.formatOnSave": true,
	},
	
	
	// TS 和 JS
	"[typescript]": {
		"editor.maxTokenizationLineLength": 2500,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnType": true
	},
	"[javascript]": {
		"editor.maxTokenizationLineLength": 2500,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnType": true
	},
	"[css]": {
		"editor.suggest.insertMode": "replace",
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnType": true
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnType": true
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"json.schemas": [
		{
			"fileMatch": [
				"/.docx.json"
			],
			"url": "https://raw.githubusercontent.com/Mehdi-Verfaillie/docx/main/src/config/.docx.schema.json"
		}
	],
	
	
	// rust
	"rust-analyzer.procMacro.enable": true,
	"[rust]": {
		"editor.formatOnSave": true,
		"editor.formatOnPaste": false,
		"editor.formatOnType": false,
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	
	
	// markdown
	"markdown-preview-enhanced.codeBlockTheme": "auto.css",
	"markdown-preview-enhanced.previewTheme": "github-light.css", // 或 github-dark.css
	"markdown-preview-enhanced.scrollSync": true, // 同步滚动
	"markdown-preview-enhanced.liveUpdate": true, // 实时更新预览
	"[markdown]": {
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.wordWrap": "on",
		"editor.quickSuggestions": {
			"comments": "off",
			"strings": "off",
			"other": "off"
		},
		// 格式化配置
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.formatOnType": false // 避免输入时过度格式化
	},
	
	
	// zig
	"zig.zls.enabled": "on",
	"[zig]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "ziglang.vscode-zig",
		"editor.stickyScroll.defaultModel": "foldingProviderModel",
		"files.eol": "\n"
	},
	// svelte
	"svelte.enable-ts-plugin": true,
	// tauri
	
	
	// go 设置
	"[go]": {
		"editor.insertSpaces": false,
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		}
	},
	"go.useLanguageServer": true,
	"go.formatTool": "goimports",
	"go.lintTool": "golangci-lint",
	"go.lintOnSave": "workspace",
	"go.vetOnSave": "workspace",
	"go.testOnSave": false,
	"go.coverOnSave": false,
	"go.toolsManagement.autoUpdate": true,
	"go.toolsEnvVars": {
		"GOPROXY": "https://goproxy.cn,direct", // go 加速下载
		"GOSUMDB": "sum.golang.google.cn" // 校验和代理
	},
	"go.lintFlags": [
		"--fast", // 快速模式
		"--enable-all", // 启用所有检查器 (可根据需要调整)
		"--disable=gochecknoglobals,gochecknoinits" // 禁用特定检查器
	],
	"go.enableCodeLens": {
		"runtest": true
	},
	"go.trace.server": "off",
	"go.diagnostic.vulncheck": "Imports",
	
	
	// vscode-icons设置
	"vsicons.presets.angular": false,
	"vsicons.dontShowNewVersionMessage": false,
	// drawio
	"hediet.vscode-drawio.resizeImages": null,
	"hediet.vscode-drawio.appearance": "automatic",
	// Hex Editor
	"hexeditor.columnWidth": 16,
	"hexeditor.showDecodedText": true,
	"hexeditor.defaultEndianness": "little",
	"hexeditor.inspectorType": "aside",
	// gitlens设置
	"gitlens.views.scm.grouped.views": {
		"commits": true,
		"branches": true,
		"remotes": true,
		"stashes": true,
		"tags": true,
		"worktrees": false,
		"contributors": true,
		"fileHistory": true,
		"repositories": true,
		"searchAndCompare": false,
		"launchpad": false
	},
	"gitlens.codeLens.recentChange.enabled": false,
	"gitlens.codeLens.authors.enabled": false,
	"gitlens.ai.model": "vscode",
	"gitlens.ai.vscode.model": "copilot:gpt-4.1",
	"git.openRepositoryInParentFolders": "never",
	"git.autofetch": true,
	"gitlens.graph.minimap.additionalTypes": [
		"stashes",
		"localBranches"
	],


	// live server 设置
	"liveServer.settings.donotVerifyTags": true,
	"liveServer.settings.donotShowInfoMsg": true,
	

	// 力扣默认登陆中国区
	"leetcode.endpoint": "leetcode-cn",


	// rainbow indent
	"indentRainbow.ignoreLinePatterns": [
		"/[ \t]* [*]/g", // lines begining with <whitespace><space>*
		"/[ \t]+[/]{2}/g" // lines begininning with <whitespace>//
	],
	"indentRainbow.indicatorStyle": "light",


	// telemetry settings
	"telemetry.telemetryLevel": "off", // 关闭遥测
	"gitlens.telemetry.enabled": false,
	"redhat.telemetry.enabled": false,


	// 文件关联设置
	"workbench.editorAssociations": {
		"{hexdiff}:/**/*.*": "hexEditor.hexedit",
		"{git,gitlens,chat-editing-snapshot-text-model,copilot,git-graph,git-graph-3}:/**/*.ui": "default",
		"*.copilotmd": "vscode.markdown.preview.editor",
		"*.pdf": "latex-workshop-pdf-hook",
		"*.pcd": "plyViewer.plyEditor",
		"*.ply": "plyViewer.plyEditor"
	},
	
	
}